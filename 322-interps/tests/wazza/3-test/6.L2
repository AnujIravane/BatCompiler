(:main
  (:main
    0 1
    (rdi <- 9)
    (rsi <- 7)
    (call allocate 2)
    ((mem rax 8) <- 13)
    ((mem rax 16) <- -1)
    ((mem rax 24) <- 11)
    ((mem rax 32) <- 13)
    ((mem rsp -8) <- :find_min_array_ret)
    (rdi <- rax)
    (call :find_min_array 1)
    :find_min_array_ret
    (rdi <- rax)
    (call print 1)
    (return))
  (:find_min_array
    1 0
    (a <- rdi)
    (a += 16) ; Start from 2nd element of the array
    (b <- (mem rdi 0))
    (b <<= 3)
    (b += rdi) ; get the address of the last element
    (rax <- (mem rdi 8)) ; Set rax to the first element before starting
    :for_start
    (cjump a <= b :for_body :for_end)
    :for_body
    (rcx <- (mem a 0))
    (cjump rcx < rax :replace :keep)
    :replace
    (rax <- (mem a 0))
    :keep
    (a += 8)
    (goto :for_start)
    :for_end
    (return))
)
