; trivial2.L1
; Determines whether the value is numeral or address

(:main
  (:main
    0 0
    (rdi <- 10)
    ((mem rsp -8) <- :is_encoded_ret_1)
    (call :is_encoded 1)
    :is_encoded_ret_1
    (rdi <- rax)
    (call print 1)
    ((mem rsp -8) <- :is_encoded_ret_2)
    (rdi <- 7)
    (call :is_encoded 1)
    :is_encoded_ret_2
    (rdi <- rax)
    (call print 1)
    (return))
  
  (:is_encoded
    1 0
    (rax <- rdi)
    (rax &= 1)
    (rax *= 2)
    (rax += 1) 
    (return)))
