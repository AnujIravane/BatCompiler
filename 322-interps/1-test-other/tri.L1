; fib.L1
; An implementation of the fibonacci function using recursion

(:main
  (:main
    0 0
    (rdi <- 10)
    ((mem rsp -8) <- :tri_ret)
    (call :tri 1)
    :tri_ret
    (rax *= 2)
    (rax += 1)
    (rdi <- rax)
    (call print 1)
    (return))
  (:tri
    1 1
    (cjump rdi = 1 :true :false)
    :true
    (rax <- 1)
    (return)
    :false
    ((mem rsp 0) <- rdi)
    (rdi -= 1)
    ((mem rsp -8) <- :tri_recur_ret)
    (call :tri 1)
    :tri_recur_ret
    (rdi <- (mem rsp 0))
    (rax += rdi)
    (return))
)
