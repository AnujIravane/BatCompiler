; heap2.L1 
; implementation of "pop" in heap
; well, array..

(:main
  (:main
    0 1
    (rdi <- 5)
    (rsi <- 7)
    (call allocate 2)
    (rdi <- rax)
    ((mem rsp 0) <- rdi)
    (call print 1)
    ((mem rsp -8) <- :pop_ret)
    (rdi <- (mem rsp 0))
    (call :pop 1)
    :pop_ret
    (rdi <- rax)
    (call print 1)
    (return))
  (:pop
    1 1
    (r8 <- (mem rdi 0)) ; size of array
    (r8 -= 1)
    (r8 *= 2)
    (r8 += 1)
    (rsi <- (mem rdi 8))
    (rdi <- r8)
    (call allocate 2)
    (return)))
